{
  "ignorePatterns": [
    {
      "pattern": "https://github.com/NVIDIA/spark-rapids/pull/*"
    },
    {
      "pattern": "https://github.com/NVIDIA/spark-rapids/issues/*"
    },
    {
      "pattern": "/docs/archives"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://docs.github.com/"],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ],
  "timeout": "15s",
  "retryOn429": true,
  "retryCount":30,
  "aliveStatusCodes": [200, 403]
} 
